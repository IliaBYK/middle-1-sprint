<div class="edit__container">
  {{{popup}}}
  {{!-- {{<a class="edit__button button" href="../../pages/mainMessanger/mainMessanger.html"></a>}} --}}
  {{{buttonToChats}}}
  <main class="edit">
    {{{avatar}}}
    {{{title}}}
    <form class="edit__form" id="edit-form">
      {{{email}}}
      {{{login}}}
      {{{first_name}}}
      {{{second_name}}}
      {{{display_name}}}
      {{{phone}}}
  
      <div class="edit__btns">
        {{#if editing}}
          {{{changeDataBtn}}}
        {{/if}}
        {{#unless editing}}
          {{{changeData}}}
          {{{changePassword}}}
          {{{logout}}}
        {{/unless}}
      </div>
    </form>
  </main>
</div>
